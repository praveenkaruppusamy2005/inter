{
  "redirects": [
    {
      "source": "/download",
      "destination": "/download/windows",
      "permanent": false
    },
    {
      "source": "/download/windows",
      "destination": "https://www.dropbox.com/scl/fi/bmibn4agoe0n2w17z5b7m/imodule-Setup-1.0.19.exe?rlkey=07yu4dzz40133fzkle4p0rb3p&st=skahdac8&dl=1",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/downloads/(.*)",
      "destination": "/downloads/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/downloads/(.*)",
      "headers": [
        { "key": "Content-Disposition", "value": "attachment" },
        { "key": "Content-Type", "value": "application/octet-stream" }
      ]
    }
  ]
}
