{
  "redirects": [
    {
      "source": "/download",
      "destination": "/download/windows",
      "permanent": false
    },
    {
      "source": "/download/windows",
      "destination": "https://www.dropbox.com/scl/fi/dmllyo8geyfgzb0cdqz20/imodule-Setup-1.0.18.exe?rlkey=fz0i47o55ymzpju6ad1hyvzcs&st=vg0i67x3&dl=1",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/downloads/(.*)",
      "destination": "/downloads/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/downloads/(.*)",
      "headers": [
        { "key": "Content-Disposition", "value": "attachment" },
        { "key": "Content-Type", "value": "application/octet-stream" }
      ]
    }
  ]
}
