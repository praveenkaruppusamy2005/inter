{
  "redirects": [
    {
      "source": "/download",
      "destination": "/download/windows",
      "permanent": false
    },
    {
      "source": "/download/windows",
      "destination": "https://www.dropbox.com/scl/fi/orosq3frex6f820176146/imodule-Setup-1.0.18.exe?rlkey=9pwk71sd4c2lrvzlkhfwz6wx6&st=gxc8an9u&dl=1",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/downloads/(.*)",
      "destination": "/downloads/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/downloads/(.*)",
      "headers": [
        { "key": "Content-Disposition", "value": "attachment" },
        { "key": "Content-Type", "value": "application/octet-stream" }
      ]
    }
  ],
  "crons": [
    {
      "path": "/api/keepalive",
      "schedule": "*/5 * * * *"
    }
  ]
}
